    <section class="mt-3">
        <div class="container">
            <h4 class="text-dark text-center mb-5">Edit profile</h4>
            <div class="box shadow">
                <div class="">
                    <form action="" class="row g-3" method="" id="profileForm">
                        <div class="col-md-4">
                            <label for="">Create Profile for</label>
                            <select class="form-control" id="profilefor" name="profilefor" required>
                                <option disabled selected value="">Select</option>
                                <option value="Groom">Groom</option>
                                <option value="Bride">Bride</option>
                            </select>
                        </div>
                        <div class="col-md-4">
                            <label for="full-name">Full Name</label>
                            <input class="form-control" type="text" id="fullname" name="fullname" required>
                        </div>
                        <div class="col-md-4">
                            <label for="city">City</label>
                            <select class="form-control" id="city" name="city" required>
                                <option disabled selected value="">Select</option>
                                <option value="Nashik">Nashik</option>
                                <option value="Pune">Pune</option>
                                <option value="Jalgaon">Jalgaon</option>
                                <option value="Kolhapur">Kolhapur</option>
                                <option value="Sambhajinanagr">Sambhajinanagr</option>
                                <option value="Chalisgaon">Chalisgaon</option>
                            </select>
                        </div>
                        <div class="col-md-4">
                            <label for="dob">Date of Birth(जन्म तारीख)</label>
                            <input type="date" class="form-control" id="dob" name="dateofbirth" required>
                        </div>
                        <div class="col-md-4">
                            <label for="education">Education</label>
                            <select class="form-control" id="education" name="education" required>
                                <option disabled selected value="">Select</option>
                                <option value="Diploma">Diploma</option>
                                <option value="Master Degree">Master Degree</option>
                                <option value="Graduation">Graduation</option>
                                <option value="12Th">12Th</option>
                                <option value="10Th">10Th</option>
                                <option value="Chalisgaon">Engineering</option>

                            </select>
                        </div>
                        <div class="col-md-4">
                            <label for="income">Income(वार्षिक उत्पन्न )</label>
                            <input type="number" class="form-control" id="income" name="income" placeholder="per annum"
                                required>
                        </div>

                        <div class="col-md-4">
                            <label for="blood-group">Blood Group</label>
                            <select class="form-control" id="bloodgroup" name="bloodgroup" required>
                                <option disabled selected value="">Select</option>
                                <option value="A positive (A+)">A positive (A+)</option>
                                <option value="A negative (A-)">A negative (A-)</option>
                                <option value="B positive (B+)">B positive (B+)</option>
                                <option value="B negative (B-)">B negative (B-)</option>
                                <option value="AB positive (AB+)">AB positive (AB+)</option>
                                <option value="AB negative (AB-)">AB negative (AB-)</option>
                                <option value="O positive (O+)">O positive (O+)</option>
                                <option value="O positive (O+)">O negative (O-)</option>

                            </select>
                        </div>

                        <div class="col-md-4">
                            <label for="spectacles">Spectacles (चष्मा )</label>
                            <select class="form-control" id="spectacles" name="spectacles" required>
                                <option disabled selected value="">Select</option>
                                <option value="yes">Yes</option>
                                <option value="no">No</option>
                            </select>
                        </div>

                        <div class="col-md-4">
                            <label for="gotra-devak">Gotra & Devak (गोत्र/देवक) </label>
                            <input class="form-control" type="text" id="gotradevak" name="gotra" required>
                        </div>

                        <div class="col-md-4">
                            <label for="birth-place">Birth Place(जन्म ठिकाण )</label>
                            <input class="form-control" type="text" id="birthplace" name="birthplace" required>
                        </div>

                        <div class="col-md-4">
                            <label for="occupation">Occupation(व्यवसाय/नोकरी )</label>
                            <input class="form-control" type="text" id="occupation" name="occupation" required>
                        </div>

                        <div class="col-md-4">
                            <label for="marital-status">Marital Status</label>
                            <select class="form-control" id="maritalstatus" name="maritalstatus" required>
                                <option disabled selected value="">Select</option>
                                <option value="single">Single</option>
                                <option value="divorced">Divorced</option>
                            </select>
                        </div>

                        <div class="col-md-4">
                            <label for="height">Height in cm (उंची सेंटी)</label>
                            <input class="form-control" type="number" id="height" name="height" required>
                        </div>

                        <div class="col-md-4">
                            <label for="occupation-city">Occupation City(व्यवसाय शहर )</label>
                            <input class="form-control" type="text" id="occupationcity" name="occupationcity" required>
                        </div>

                        <div class="col-md-4">
                            <label for="complexion">Complexion(वर्ण)</label>
                            <input class="form-control" type="text" id="complexion" name="complexion" required>
                        </div>

                        <div class="col-md-4">
                            <label for="mangal">Mangal(मंगळ)</label>
                            <select class="form-control" id="mangal" name="mangal" required>
                                <option disabled selected value="">Select</option>
                                <option value="yes">Yes</option>
                                <option value="no">No</option>

                            </select>
                        </div>
                        <div class="col-md-4">
                            <label for="resident-city">Resident City(राहते शहर )</label>
                            <select class="form-control" id="residentcity" name="residentcity" required>
                                <option disabled selected value="">Select</option>
                                <option value="Nashik">Nashik</option>
                                <option value="Pune">Pune</option>
                                <option value="Jalgaon">Jalgaon</option>
                                <option value="Kolhapur">Kolhapur</option>
                                <option value="Sambhajinanagr">Sambhajinanagr</option>
                                <option value="Chalisgaon">Chalisgaon</option>
                            </select>
                        </div>

                        <div class="row mt-3">

                            <div class="col-md-12 mb-4">
                                <div class="row">
                                    <div class="col-md-6">
                                        <label for="age-difference"> Email id </label>
                                        <input class="form-control" type="email" id="email" name="email" required>
                                    </div>
                                    <div class="col-md-6">
                                        <label for="age-difference">Mobile number</label>
                                        <input class="form-control" type="number" id="mobile" name="mobile" required>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <h6 class="text-dark">Family Information</h6>
                                <div class="row py-4 border-top">
                                    <div class="col-md-4">
                                        <label for="father-name">Father's Name</label>
                                        <input class="form-control" type="text" id="fathername" name="fathername"
                                            required>
                                    </div>

                                    <div class="col-md-4">
                                        <label for="mother-name">Mother's Name</label>
                                        <input class="form-control" type="text" id="mothername" name="mothername"
                                            required>
                                    </div>

                                    <div class="col-md-4">
                                        <label for="maternal-uncle">Maternal Uncle(मामकूल )</label>
                                        <input class="form-control" type="text" id="maternaluncle" name="maternaluncle"
                                            required>
                                    </div>

                                    <div class="col-md-4">
                                        <label for="native-place">Native Place</label>
                                        <input class="form-control" type="text" id="nativeplace" name="nativeplace"
                                            required>
                                    </div>

                                    <div class="col-md-4">
                                        <label for="family-wealth">Family Wealth</label>
                                        <select class="form-control" id="citywealth" name="citywealth" required>
                                            <option disabled selected value="">Select</option>
                                            <option value="Nashik">Nashik</option>
                                            <option value="Pune">Pune</option>
                                            <option value="Jalgaon">Jalgaon</option>
                                            <option value="Kolhapur">Kolhapur</option>
                                            <option value="Sambhajinanagr">Sambhajinanagr</option>
                                            <option value="Chalisgaon">Chalisgaon</option>

                                        </select>
                                    </div>

                                    <div class="col-md-4">
                                        <label for="parents-residing-in">Parents Residing In (City)</label>
                                        <select class="form-control" id="parentcity" name="parentcity" required>
                                            <option disabled selected value="">Select</option>
                                            <option value="Nashik">Nashik</option>
                                            <option value="Pune">Pune</option>
                                            <option value="Jalgaon">Jalgaon</option>
                                            <option value="Kolhapur">Kolhapur</option>
                                            <option value="Sambhajinanagr">Sambhajinanagr</option>
                                            <option value="Chalisgaon">Chalisgaon</option>

                                        </select>
                                    </div>

                                    <div class="col-md-4">
                                        <label for="sister">Sister(Yes/No) </label>
                                        <select class="form-control" id="sister" name="sister" required>
                                            <option disabled selected value="">Select</option>
                                            <option value="yes">Yes</option>
                                            <option value="no">No</option>
                                        </select>
                                    </div>

                                </div>

                                <div class="row border-top">
                                    <h6 class="text-dark p-3">Expectations</h6>
                                    <div class="col-md-12">
                                        <div class="row">
                                            <div class="col-md-4">
                                                <label for="age-difference">Age Difference Upto:(In Year) </label>
                                                <input class="form-control" type="text" id="agedifference"
                                                    name="agedifference" required>
                                            </div>
                                            <div class="col-md-4">
                                                <label for="preferred-city">Preferred City:</label>
                                                <select class="form-control" id="preferredcity" name="preferredcity"
                                                    required>
                                                    <option disabled selected value="">Select</option>
                                                    <option value="Nashik">Nashik</option>
                                                    <option value="Pune">Pune</option>
                                                    <option value="Jalgaon">Jalgaon</option>
                                                    <option value="Kolhapur">Kolhapur</option>
                                                    <option value="Sambhajinanagr">Sambhajinanagr</option>
                                                    <option value="Chalisgaon">Chalisgaon</option>

                                                </select>
                                            </div>
                                            <div class="col-md-4">
                                                <label for="expected-height">Expected Height:</label>
                                                <input class="form-control" type="number" id="expectedheight"
                                                    name="expectedheight" required>
                                            </div>
                                            <div class="col-md-4">
                                                <label for="education-dropdown">Education:</label>
                                                <select class="form-control" id="herhiseducation" name="herhiseducation"
                                                    required>
                                                    <option disabled selected value="">Select</option>
                                                    <option value="Diploma">Diploma</option>
                                                    <option value="Master Degree">Master Degree</option>
                                                    <option value="Graduation">Graduation</option>
                                                    <option value="12Th">12Th</option>
                                                    <option value="10Th">10Th</option>
                                                    <option value="Chalisgaon">Engineering</option>

                                                </select>
                                            </div>
                                            <div class="col-md-4">
                                                <label for="occupations">Occupations:</label>
                                                <input class="form-control" type="text" id="herhisoccupation"
                                                    name="herhisoccupation" required>
                                            </div>
                                            <div class="col-md-4">
                                                <label for="parent-residing-in">Parent Residing In: (City)</label>
                                                <input class="form-control" type="text" id="herhisparentresidence"
                                                    name="herhisparentresidence" required>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="mt-4 position-sticky bg-white w-100" style="bottom: 10px;">
                            <div class="border-top" style="text-align: end;">
                                  <button class="btn btn-lg btn-secondary mt-4">Edit</button>
                                  <button class="btn btn-lg btn-dark mt-4 ms-3">save</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>

        </div>

    </section>



